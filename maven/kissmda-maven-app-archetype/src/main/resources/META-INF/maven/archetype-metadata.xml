<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
	name="kissmda-uml2-project"
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<requiredProperties>
		<requiredProperty key="projectName">
			<defaultValue>UML2 KissMDA Project</defaultValue>
		</requiredProperty>
		<requiredProperty key="projectDescription">
			<defaultValue>UML2 KissMDA Project Starter with Maven</defaultValue>
		</requiredProperty>
	</requiredProperties>
	<fileSets>
		<fileSet filtered="true" encoding="UTF-8" packaged="true">
			<directory>src/main/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8" packaged="false">
			<directory>src/main/resources</directory>
			<includes>
				<include>*.properties</include>
			</includes>
		</fileSet>
		<fileSet filtered="false" encoding="UTF-8" packaged="false">
			<directory>src/main/resources/model</directory>
			<includes>
				<include>*.mdzip</include>
			</includes>
		</fileSet>
		<fileSet filtered="false" encoding="UTF-8" packaged="false">
			<directory>src/main/resources/model/emf</directory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet filtered="false" encoding="UTF-8" packaged="false">
			<directory>src/main/resources/model/profiles</directory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8" packaged="true">
			<directory>src/test/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
		</fileSet>
	</fileSets>
</archetype-descriptor>